{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "types": ["vite/client"] // https://vitejs.dev/guide/features.html#client-types
  },
  "include": ["src", "static-site-generation.js"]
}
